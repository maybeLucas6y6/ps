exA_1_c = function(lambda) {
k = 0;
while (ppois(k, lambda) < (1 - 1e-6)) {
k = k + 1;
}
return(k);
}
exA_2_a = function() {
file = read.csv("note_PS.csv", header = T, sep = ',');
prob = file[['P']];
stat = file[['S']];
fr_abs_prob = as.vector(table(prob));
fr_abs_stat = as.vector(table(stat));
print(fr_abs_prob);
print(fr_abs_stat);
fr_rel_prob = fr_abs_prob / length(prob);
fr_rel_stat = fr_abs_stat / length(stat);
print(fr_rel_prob);
print(fr_rel_stat);
mean_prob = mean(prob);
mean_stat = mean(stat);
print(mean_prob);
print(mean_stat);
}
