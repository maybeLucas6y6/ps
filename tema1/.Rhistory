exA_2_b = function(filename, esantion) {
file = read.csv(filename, header = T, sep = ',');
if (esantion == 'P') {
x = file[['P']]
}
else if (esantion == 'S') {
x = file[['S']]
}
m = mean(x);
s = sd(x);
v = vector();
h = 1;
for (i in 1:length(x)) {
if (x[i] < m + 2 * s && x[i] > m - 2 * s) {
v[h] = x[i];
h = h + 1;
}
}
print(as.vector(table(v)));
hist(v, right = TRUE, breaks = (min(v)-1):max(v));
}
